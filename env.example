# Google Cloud Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCP_LOCATION=global

# Service Account Authentication (recommended over personal credentials)
# Set this to the path of your service account JSON key file
GOOGLE_APPLICATION_CREDENTIALS=service-account-key.json

# Agent Configuration (optional - can be created via UI)
DEFAULT_AGENT_ID=datachat_agent
DEFAULT_CONVERSATION_ID=

# BigQuery Data Source (default: public e-commerce dataset)
BQ_PROJECT_ID=bigquery-public-data
BQ_DATASET_ID=thelook_ecommerce
BQ_TABLE_IDS=orders,order_items,products,users

# Feature Flags
ENABLE_PYTHON_ANALYSIS=true
ENABLE_CHART_RENDERING=true
CONVERSATION_MODE=stateful
